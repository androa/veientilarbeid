{
    "name": "veientilarbeid",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "cross-env REACT_APP_MOCK=true craco start",
        "start:micro": "cross-env REACT_APP_MOCK=true REACT_APP_MICRO='true' craco start",
        "build:micro": "cross-env REACT_APP_MICRO=true npm run build",
        "build": "REACT_APP_SENTRY_RELEASE=$(sentry-cli releases propose-version) craco build",
        "lint": "eslint --ext=jsx,js,ts,tsx ./src",
        "lint:fix": "eslint --ext=jsx,js,ts,tsx ./src --fix",
        "test": "npm run lint && craco test --env=jsdom",
        "test-ci": "npm run lint && CI=true craco test --env=jsdom",
        "test:coverage": "npm test -- --coverage --watchAll=false",
        "deploy": "npm run build && gh-pages -d build",
        "prettier": "prettier --write 'src/**/*.ts' 'src/**/*.tsx' 'src/**/*.less'",
        "refresh": "rm -rf node_modules && rm package-lock.json && npm i",
        "now-build": "cross-env REACT_APP_MOCK=true npm run build",
        "snyk-protect": "snyk protect",
        "prepare": "npm run snyk-protect",
        "sentry-release": "SENTRY_RELEASE=$(sentry-cli releases propose-version) node scripts/sentry-release",
        "postinstall": "husky install"
    },
    "dependencies": {
        "@alheimsins/uker-fra-dato": "1.0.4",
        "@alheimsins/virkedager": "2.1.1",
        "@craco/craco": "6.4.2",
        "@navikt/ds-css": "^0.11.5",
        "@navikt/ds-icons": "0.6.3",
        "@navikt/ds-react": "^0.13.3",
        "@navikt/fnrvalidator": "1.1.4",
        "@sentry/browser": "6.15.0",
        "@sentry/react": "6.15.0",
        "amplitude-js": "8.13.1",
        "classnames": "2.3.1",
        "core-js": "3.19.2",
        "craco-less": "1.20.0",
        "gsap": "3.8.0",
        "html-react-parser": "1.4.1",
        "intersection-observer": "0.12.0",
        "lodash.throttle": "4.1.1",
        "nav-frontend-alertstriper": "4.0.1",
        "nav-frontend-alertstriper-style": "3.0.1",
        "nav-frontend-chevron": "1.0.30",
        "nav-frontend-chevron-style": "1.0.4",
        "nav-frontend-core": "6.0.1",
        "nav-frontend-ekspanderbartpanel": "4.0.2",
        "nav-frontend-ekspanderbartpanel-style": "2.0.1",
        "nav-frontend-ikoner-assets": "3.0.1",
        "nav-frontend-ikonknapper": "2.1.2",
        "nav-frontend-js-utils": "1.0.20",
        "nav-frontend-knapper": "3.1.2",
        "nav-frontend-knapper-style": "2.1.1",
        "nav-frontend-lenkepanel": "2.0.1",
        "nav-frontend-lenkepanel-style": "2.0.1",
        "nav-frontend-lenker": "2.0.1",
        "nav-frontend-lenker-style": "2.0.1",
        "nav-frontend-paneler": "3.0.1",
        "nav-frontend-paneler-style": "2.0.1",
        "nav-frontend-popover": "2.0.3",
        "nav-frontend-popover-style": "2.0.1",
        "nav-frontend-skjema": "4.0.5",
        "nav-frontend-skjema-style": "3.0.2",
        "nav-frontend-spinner": "3.0.1",
        "nav-frontend-spinner-style": "1.0.2",
        "nav-frontend-typografi": "4.0.1",
        "nav-frontend-typografi-style": "2.0.1",
        "prop-types": "15.7.2",
        "react": "17.0.2",
        "react-app-polyfill": "2.0.0",
        "react-collapse": "5.1.1",
        "react-dom": "17.0.2",
        "react-ga": "3.3.0",
        "react-in-viewport": "1.0.0-alpha.20"
    },
    "devDependencies": {
        "@sentry/cli": "1.71.0",
        "@testing-library/dom": "8.11.1",
        "@testing-library/jest-dom": "5.16.0",
        "@testing-library/react": "12.1.2",
        "@testing-library/user-event": "13.5.0",
        "@types/amplitude-js": "8.0.2",
        "@types/merge-deep": "3.0.0",
        "@types/react": "17.0.37",
        "@types/react-collapse": "5.0.1",
        "@types/react-dom": "17.0.11",
        "cross-env": "7.0.3",
        "husky": "7.0.4",
        "less": "4.1.2",
        "lint-staged": "12.1.2",
        "merge-deep": "3.0.3",
        "msw": "0.36.0",
        "prettier": "2.5.1",
        "react-scripts": "4.0.3",
        "snyk": "1.783.0",
        "typescript": "4.4.4"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie < 11",
        "not op_mini all"
    ],
    "prettier": {
        "printWidth": 120,
        "singleQuote": true,
        "tabWidth": 4
    },
    "lint-staged": {
        "*.{js,jsx,json,md,ts,tsx}": "prettier --write"
    },
    "snyk": true
}
