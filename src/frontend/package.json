{
  "name": "veientilarbeid",
  "homepage": "/veientilarbeid",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "2.2.5",
    "nav-frontend-chevron-style": "^0.3.4",
    "nav-frontend-core": "4.0.5",
    "nav-frontend-grid": "0.1.10",
    "nav-frontend-grid-style": "0.2.4",
    "nav-frontend-lenkepanel": "0.3.16",
    "nav-frontend-lenkepanel-style": "0.3.6",
    "nav-frontend-typografi": "1.0.12",
    "nav-frontend-typografi-style": "1.0.7",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-intl": "2.4.0",
    "react-redux": "5.0.6",
    "react-scripts-ts": "2.8.0",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0"
  },
  "scripts": {
    "copyfiles": "copyfiles -u 1 build/**/*.* ../main/webapp",
    "delete-static": "node scripts/deletestatic.js",
    "start:ts": "react-scripts-ts start",
    "build:ts": "react-scripts-ts build",
    "deploy": "npm run build && gh-pages -d build",
    "test": "react-scripts-ts test --env=jsdom",
    "lint": "tslint ./src/**/*.ts*",
    "lint:fix": "tslint ./src/**/*.ts* --fix",
    "test:coverage": "npm test -- --coverage",
    "prettier": "prettier --write --single-quote --tab-width 4 src/**/*.ts src/**/*.tsx src/**/*.less",
    "eject": "react-scripts-ts eject",
    "start:less": "autoless --source-map --intervall 100 ./src ./public",
    "build:less": "node ./node_modules/less/bin/lessc/ src/index.less ./public/index.css",
    "start": "npm-run-all -p start:*",
    "build:tekster": "node ./src/utils/build-tekster.js",
    "build": "npm run delete-static npm run build:less && npm run build:ts && npm run build:tekster"
  },
  "devDependencies": {
    "@types/jest": "21.1.8",
    "@types/node": "8.0.53",
    "@types/react": "16.0.25",
    "@types/react-dom": "16.0.3",
    "@types/react-intl": "2.3.2",
    "@types/react-redux": "5.0.10",
    "autoless": "0.1.7",
    "copyfiles": "^1.2.0",
    "less": "2.7.3",
    "npm-run-all": "4.0.2"
  }
}
